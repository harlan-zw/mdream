{
  "name": "@mdream/llms-db",
  "type": "module",
  "version": "0.8.5",
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "types": "./dist/index.d.mts"
    },
    "./cli": {
      "import": "./dist/cli.mjs",
      "types": "./dist/cli.d.mts"
    }
  },
  "bin": {
    "mdream-db": "./dist/cli.mjs"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "obuild",
    "typecheck": "tsc --noEmit",
    "test": "vitest",
    "test:attw": "attw --pack .",
    "run": "node --env-file=.env ./dist/cli.mjs run",
    "db:generate": "node --env-file=.env drizzle-kit generate",
    "db:migrate": "node --env-file=.env drizzle-kit migrate",
    "db:studio": "node --env-file=.env drizzle-kit studio"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.850.0",
    "@clack/prompts": "^0.11.0",
    "@libsql/client": "^0.15.10",
    "@mdream/crawl": "workspace:*",
    "better-sqlite3": "^12.2.0",
    "drizzle-orm": "^0.44.3",
    "pathe": "^2.0.3",
    "ufo": "^1.6.1",
    "unstorage": "^1.16.1"
  },
  "devDependencies": {
    "@types/better-sqlite3": "^7.6.13",
    "drizzle-kit": "^0.31.4"
  }
}
