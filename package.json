{
  "name": "downstream",
  "type": "module",
  "version": "4.3.0",
  "description": "Convert HTML to Markdown using ultrahtml",
  "author": {
    "name": "Harlan Wilton",
    "email": "harlan@harlanzw.com",
    "url": "https://harlanzw.com/"
  },
  "license": "MIT",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs"
    },
    "./cli": "./dist/cli.mjs"
  },
  "main": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "bin": "./bin/downstream.mjs",
  "files": [
    "bin",
    "dist"
  ],
  "scripts": {
    "ts": "node  --experimental-strip-types",
    "test:github": " cat test/github-markdown.html | node ./bin/downstream.mjs > test/github-markdown.md",
    "build": "unbuild",
    "dev:prepare": "unbuild --stub",
    "test": "vitest"
  },
  "dependencies": {
    "cac": "^6.7.14"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^4.12.0",
    "bumpp": "^10.1.0",
    "eslint": "^9.24.0",
    "jsdom": "^26.1.0",
    "showdown": "^2.1.0",
    "turndown": "^7.2.0",
    "typescript": "5.8.3",
    "unbuild": "^2.0.0",
    "vitest": "^3.1.1"
  }
}
